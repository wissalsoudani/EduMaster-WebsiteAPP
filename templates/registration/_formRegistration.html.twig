{{ form_start(form,{attr:{novalidate:'novalidate'}}) }}
<div class="form-group">
    <label for="username" ><i class="zmdi zmdi-account material-icons-name"></i></label>
    {{ form_widget (form.login,{'attr':{'class':'form-control' ,'placeholder': "Username"}})  }}
    
</div>
<div class="form-group">
    <label for="pass"><i class="zmdi zmdi-lock"></i></label>
     {{ form_widget (form.Password.first,{'attr':{'class':'form-control','placeholder': "Your password"}})  }}
</div>
<div class="form-group">
    <label for="pass"><i class="zmdi zmdi-lock"></i></label>
     {{ form_widget (form.Password.second,{'attr':{'class':'form-control','placeholder': "Re enter password"}})  }}
</div>
<div class="form-group">
    <label for="nom"><i class="zmdi account-box"></i></label>
    {{ form_widget (form.nom,{'attr':{'class':'form-control' ,'placeholder': "Your family name"}})  }}
</div>
<div class="form-group">
    <label for="prenom"><i class="zmdi account-box-o"></i></label>
    {{ form_widget (form.prenom,{'attr':{'class':'form-control' ,'placeholder': "Your first name"}})  }}
</div>
<div class="form-group">
    <label for="niveau"><i class="zmdi accounts-alt"></i></label>
    {{ form_widget (form.niveau,{'attr':{'class':'form-control' ,'placeholder': "Your level"}})  }}
</div>
<div class="form-group">
    <label for="email"><i class="zmdi zmdi-email"></i></label>
    {{ form_widget (form.mail,{'attr':{'class':'form-control' ,'placeholder': "Your email"}})  }}
</div>

<div class="form-group form-button">
    <input type="submit" name="signup" id="signup" class="form-submit" value="Register"/>
</div>
{{ form_end(form) }}
